{
    "name": "ofmadsen/taupe",
    "type": "library",
    "description": "Dependency Injector for PHP",
    "keywords": ["di", "ioc"],
    "homepage": "https://github.com/ofmadsen/taupe",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Finn Madsen",
            "email": "mail@ofmadsen.com",
            "homepage": "http://www.ofmadsen.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Taupe\\": "src/Taupe/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }
}
